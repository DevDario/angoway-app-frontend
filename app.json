{
  "expo": {
    "name": "angoway-app-frontend",
    "slug": "angoway-app-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/angoway-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "angoway",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBFCFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-screen.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon-100x100.png"
    }
  }
}
